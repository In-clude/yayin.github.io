    <title>关于小帅（内涵多重福利）</title>
    <link href="http://example.com/2021/05/06/%E5%85%B3%E4%BA%8E%E5%B0%8F%E5%B8%85%EF%BC%88%E5%86%85%E6%B6%B5%E5%A4%9A%E9%87%8D%E7%A6%8F%E5%88%A9%EF%BC%89/"/>
    <id>http://example.com/2021/05/06/%E5%85%B3%E4%BA%8E%E5%B0%8F%E5%B8%85%EF%BC%88%E5%86%85%E6%B6%B5%E5%A4%9A%E9%87%8D%E7%A6%8F%E5%88%A9%EF%BC%89/</id>
    <published>2021-05-06T14:42:41.000Z</published>
